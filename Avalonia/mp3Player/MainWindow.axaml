<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="mp3Player.MainWindow"
        Title="mp3Player">
    <StackPanel>
        <Button Click="chooseFile" Width="200" Height="40">Wybierz Plik</Button>
        <Button Click="playButton" Width="200" Height="40">Odtwarzaj</Button>
        <Button Click="pauseButton" Width="200" Height="40">Zapauzuj</Button>
        <Button Click="stopButton" Width="200" Height="40">Stop</Button>
        <TextBox Name="CurrentAudio" Width="200" Height="20" IsReadOnly="True"></TextBox>
    </StackPanel>
</Window>
